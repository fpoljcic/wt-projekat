@objects
	meni 		css     .meni
	meni_item_1	xpath	//div[@class='meni']/ul/li[1]
	meni_item_2	xpath	//div[@class='meni']/ul/li[2]
	logo 		css     .logo
	ul 			css		.meni ul
	sadrzaj 	css		.sadrzaj
	slika_1 	xpath	//div[@class='sadrzaj']/img[1]
	slika_2 	xpath	//div[@class='sadrzaj']/img[2]
	slika_3 	xpath	//div[@class='sadrzaj']/img[3]
	slika_4 	xpath	//div[@class='sadrzaj']/img[4]
= Meni section =
	logo:
		inside partly meni -40 to -38px top
		width 95 to 97% of viewport/width
		height 80 to 84px
		centered horizontally inside viewport
	meni:
		above slika_1 0px
		width 95 to 97% of viewport/width
		centered horizontally inside viewport
	ul:
		inside meni
		contains meni_item_1
		inside viewport 7 to 11px left
		below logo
		centered horizontally inside meni 1px
	meni_item_1:
		aligned vertically all meni_item_2
		above meni_item_2 0px
		height 12 to 16px
		inside ul 0px top, 2 to 6px left
	meni_item_2:
		inside ul 2 to 6px left
		height 12 to 16px
= Main section =
	sadrzaj:
		width 95 to 97% of viewport/width
		below meni
		centered horizontally inside viewport
		# inside viewport 7 to 11px left
	slika_1:
		inside sadrzaj 0px top
		inside sadrzaj 0px left
		width 30 to 32 % of viewport/width
		below meni 0px
		aligned vertically all slika_2 1px
	slika_2:
		aligned vertically all slika_3 1px
		width 30 to 32 % of viewport/width
		below slika_1 0 to 1px
	slika_3:
		aligned vertically all slika_4 1px
		width 30 to 32 % of viewport/width
		below slika_2 0 to 1px
	slika_4:
		width 30 to 32 % of viewport/width
		below slika_3 0 to 1px