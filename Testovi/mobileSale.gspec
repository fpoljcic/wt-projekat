@objects
	meni 		css     .meni
	meni_item_1	xpath	//div[@class='meni']/ul/li[1]
	meni_item_2	xpath	//div[@class='meni']/ul/li[2]
	logo 		css     .logo
	ul 			css		.meni ul
	sadrzaj 	css		.sadrzaj
	tabela 		css		.tabelaSala
	headerRedni	css 	.headerRedni
	headerSprat	css 	td.headerSprat
= Meni section =
	logo:
		width 95 to 97% of viewport/width
		height 80 to 84px
		centered horizontally inside viewport
	meni:
		inside partly logo 37 to 41px top
		above sadrzaj 5 to 9px
		width 95 to 97% of viewport/width
		centered horizontally inside viewport
	ul:
		inside meni
		contains meni_item_1
		inside viewport 7 to 11px left
		below logo
		centered horizontally inside meni 1px
	meni_item_1:
		aligned vertically all meni_item_2
		above meni_item_2 0px
		height 14 to 18px
		inside ul 0px top, 2 to 6px left
	meni_item_2:
		inside ul 2 to 6px left
		height 14 to 18px
= Main section =
	sadrzaj:
		width 95 to 97% of viewport/width
		below meni
		contains tabela
		centered horizontally inside viewport
	tabela:
		inside sadrzaj 13 to 17px top bottom
		centered horizontally inside sadrzaj
		centered vertically inside sadrzaj
		aligned vertically centered meni_item_1 1px
	headerRedni:
		absent
	headerSprat:
		absent